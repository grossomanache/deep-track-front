{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "test": "jest --watchAll=false",
    "test-coverage": "jest --coverage --watchAll=false --passWithNoTests=true",
    "personal-test": "jest -- --coverage --watchAll",
    "update-test": "jest -- --u --coverage"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@fontsource/roboto": "^4.5.7",
    "@mui/material": "^5.9.0",
    "jest": "^28.1.3",
    "next": "12.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.2",
    "husky": "^7.0.0",
    "typescript": "4.7.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "pages/**/*.ts",
      "pages/**/*.tsx",
      "!src/index.tsx",
      "!src/reportWebVitals.ts",
      "!src/setupTests.ts",
      "!src/react-app-env.d.ts",
      "!coverage/*/.ts",
      "!src/redux/interfaces/*.ts",
      "!src/**/*.test.ts",
      "!src/**/*.test.ts.snap",
      "!src/**/*.test.tsx.snap",
      "!src/**/*.test.tsx.snap"
    ]
  }
}
